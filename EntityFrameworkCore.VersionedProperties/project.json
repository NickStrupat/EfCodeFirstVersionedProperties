{
	"version": "1.0.0-*",

	"configurations": {
		"Debug": {
			"buildOptions": {
				"define": [ "DEBUG", "TRACE", "EF_CORE" ]
			}
		},
		"Release": {
			"buildOptions": {
				"define": [ "RELEASE", "TRACE", "EF_CORE" ],
				"optimize": true
			}
		}
	},

	"dependencies": {
		"EntityFrameworkCore.Triggers": "1.0.5",
		"Z.EntityFramework.Plus.EFCore": "1.4.10"
	},

	"frameworks": {
		"net451": {
			"buildOptions": { "outputName": "EntityFrameworkCore.VersionedProperties.net451" }
		},
		"netstandard1.3": {
			"buildOptions": { "outputName": "EntityFrameworkCore.VersionedProperties.netstandard1.3" },
			"dependencies": {
				"NETStandard.Library": "1.6.1"
			},
			"imports": "dnxcore50"
		}
	}
}
